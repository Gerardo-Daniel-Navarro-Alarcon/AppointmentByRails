<div id="<%= dom_id service %>">
  <p>
    <strong>Name:</strong>
    <%= service.name %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= service.description %>
  </p>

  <p>
    <strong>Category:</strong>
    <%= service.category %>
  </p>

  <p>
    <strong>Price:</strong>
    <%= service.price %>
  </p>

  <p>
    <strong>Active:</strong>
    <%= service.active %>
  </p>

  <p>
    <strong>Created at:</strong>
    <%= service.created_at %>
  </p>

  <p>
    <strong>Updated at:</strong>
    <%= service.updated_at %>
  </p>

</div>
